<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="report/customreports/db" VERSION="20210719" COMMENT="XMLDB file for Moodle report/customreports"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="custom_reports" COMMENT="Table Stores the title of report and the file name">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="report_title" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="file_name" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
